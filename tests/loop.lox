// simple while
var a = 0;
while (a < 5) {
  print "hello";
  a = a + 1;
}
print a;


// no initializer
var a = 1;
for (; a < 10; a = a + a) {
  print a;
}

// no increment
for (var a = 1; a < 10;) {
  print a;
  a = a + a;
}

// fibonacci 
var a = 0;
var temp;

for (var b = 1; a < 10; b = temp + b) {
  print a;
  temp = a;
  a = b;
}
